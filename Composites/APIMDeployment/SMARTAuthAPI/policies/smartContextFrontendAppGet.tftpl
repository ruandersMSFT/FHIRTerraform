<policies>
    <inbound>
        <base />
        <set-backend-service id="apim-generated-policy" backend-id="contextFrontendAppBackend" />
        <rewrite-uri template="@((context.Request.OriginalUrl.Path).Replace("auth/context/", ""))" copy-unmatched-params="true" />
    </inbound>
    <backend>
        <base />
    </backend>
    <outbound>
        <base />
    </outbound>
    <on-error>
        <base />
    </on-error>
</policies>
